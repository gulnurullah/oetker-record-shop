{
    "name": "oetker/record-shop",
    "type": "project",
    "description": "Oetker record shop project",
    "license": "proprietary",
    "require": {
        "php": "^7.4.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "beberlei/doctrineextensions": "^1.2",
        "jms/serializer-bundle": "^3.7",
        "nelmio/api-doc-bundle": "^3.3",
        "nelmio/cors-bundle": "^2.0",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/asset": "5.0.*",
        "symfony/console": "5.0.*",
        "symfony/dotenv": "5.0.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.0.*",
        "symfony/framework-bundle": "5.0.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "5.0.*",
        "symfony/yaml": "5.0.*"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "fzaninotto/faker": "^1.9",
        "phpstan/phpstan-symfony": "^0.12.6",
        "symfony/browser-kit": "5.0.*",
        "symfony/maker-bundle": "^1.19",
        "symfony/phpunit-bridge": "^5.1",
        "symfony/profiler-pack": "^1.0",
        "symfony/validator": "5.0.*",
        "symfony/var-dumper": "5.0.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "setup:dev": [
            "php bin/console doctrine:database:create --if-not-exists",
            "php bin/console doctrine:migrations:migrate"
        ],
        "fixture:load": "php bin/console doctrine:fixtures:load",
        "build:lint": [
            "@php bin/console lint:yaml config --parse-tags",
            "@php bin/console lint:yaml src --parse-tags"
        ],
        "build:test": [
            "@composer validate",
            "./bin/phpunit"
        ],
        "php:unit": "./bin/phpunit",
        "php:stan": "vendor/bin/phpstan analyse -l 1 src"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.0.*"
        }
    }
}
